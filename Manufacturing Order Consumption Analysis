SELECT 
	A.MTITNO AS "Product number" 
	,A.MTBANO AS "Lote"
	,A.MTRIDN AS "Order number"
	,C.MMITTY AS "Item type"
	,SUM(A.MTTRQT) AS "Quantity"
FROM 
	MVXJDTA.MITTRA A,  MVXJDTA.MITMAS C 
 WHERE
	A.MTITNO = C.MMITNO	
	AND MTCONO = '1' 
	AND MTRIDN = '0014132656'
	AND MTTTID = 'WOM'
	AND NOT MTBANO = ''
	AND C.MMITTY LIKE '1%'
GROUP BY MTITNO, MTBANO, MTRIDN, MMITTY
